	x.[RepNo],
	sr.[RepName],
	[SortSeq],
	[RepDlr],
	[RepPct],
	[Cowner],
	[CorpUpd],
	[Division],
	[OldCustNo],
	[GSMStatus],
	[CustomerStatus],
	[CStatus],
	[MCNO],
