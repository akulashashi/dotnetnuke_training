	[ExpiryDate] datetime NULL, 

	[ViewOrder] int NULL, 
	[UpdatedByUserId] int NULL, 
	[UpdatedDate] datetime NULL, 
	[CreatedByUserId] int NULL, 
	[CreatedDate] datetime NULL, 

	[RatingVotes] int NULL, 
	[RatingTotal] int NULL

) ON [PRIMARY]
GO


