if exists (select * from dbo.sysobjects where id = object_id(N'{databaseOwner}[{objectQualifier}bhattji_UpdateXYZ70Rating]') and OBJECTPROPERTY(id, N'IsProcedure') = 1)
drop procedure {databaseOwner}[{objectQualifier}bhattji_UpdateXYZ70Rating]
GO

CREATE PROCEDURE {databaseOwner}[{objectQualifier}bhattji_UpdateXYZ70Rating]
	@ItemId      int,
	@RatingTotal   int

AS

UPDATE [{objectQualifier}bhattji_XYZ70s]
SET	[RatingVotes] = CASE WHEN [RatingVotes] IS NULL THEN 1 ELSE ([RatingVotes] + 1) END
WHERE ItemId = @ItemId

UPDATE [{objectQualifier}bhattji_XYZ70s]
SET	[RatingTotal] = CASE WHEN RatingTotal IS NULL THEN @RatingTotal ELSE ([RatingTotal] + @RatingTotal) END
WHERE [ItemId] = @ItemId

GO



